<section>
    <div class="container">
               <table class="table">
  <thead>
    <tr>
      <th scope="col">No</th>
      <th scope="col">Tittle</th>
      <th scope="col">Category</th>
      <th scope="col">Description</th>
      <th>Image</th>
    </tr>
  </thead>
  <tbody>
  {{#each products}}
    <tr>
      <th scope="row">1</th>
      <td>{{this.name}}</td>
      <td>{{this.category}}</td>
      <td>{{this.Discription}}</td>
      <td><img style="width: 65px;height:55px;" src="{{this.Image}}" alt=""></td>
    </tr>
    {{/each}}

  
  </tbody>
</table>
    </div>
</section>